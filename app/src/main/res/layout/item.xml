<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="19dp"
        android:text="CEP:"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Logradouro:"
        app:layout_constraintStart_toStartOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Complemento:"
        app:layout_constraintStart_toStartOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bairro:"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="IBGE"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="DDD:"
        app:layout_constraintStart_toStartOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Localidade:"
        app:layout_constraintStart_toStartOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="UF:"
        app:layout_constraintStart_toStartOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/txtitem_CEP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="9dp"
        android:layout_marginTop="19dp"
        android:hint="CEP"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtitem_DDD"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="ddd"
        app:layout_constraintStart_toStartOf="@+id/txtitem_Bairro"
        app:layout_constraintTop_toBottomOf="@+id/txtitem_IBGE" />

    <TextView
        android:id="@+id/txtitem_Localidade"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="localidade"
        app:layout_constraintStart_toStartOf="@+id/txtitem_DDD"
        app:layout_constraintTop_toBottomOf="@+id/txtitem_DDD" />

    <TextView
        android:id="@+id/txtitem_Bairro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="bairro"
        app:layout_constraintStart_toStartOf="@+id/txtitem_Complemento"
        app:layout_constraintTop_toBottomOf="@+id/txtitem_Complemento" />

    <TextView
        android:id="@+id/txtitem_IBGE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="ibge"
        app:layout_constraintEnd_toEndOf="@+id/txtitem_UF"
        app:layout_constraintStart_toEndOf="@+id/txtitem_UF"
        app:layout_constraintTop_toBottomOf="@+id/txtitem_Bairro" />

    <TextView
        android:id="@+id/txtitem_UF"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="Uf"
        app:layout_constraintStart_toStartOf="@+id/txtitem_Localidade"
        app:layout_constraintTop_toBottomOf="@+id/txtitem_Localidade" />

    <TextView
        android:id="@+id/txtitem_Complemento"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="complemento"
        app:layout_constraintStart_toStartOf="@+id/txtitem_Logradouro"
        app:layout_constraintTop_toBottomOf="@+id/txtitem_Logradouro" />

    <TextView
        android:id="@+id/txtitem_Logradouro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="logradouro"
        app:layout_constraintStart_toStartOf="@+id/txtitem_CEP"
        app:layout_constraintTop_toBottomOf="@+id/txtitem_CEP" />

</androidx.constraintlayout.widget.ConstraintLayout>